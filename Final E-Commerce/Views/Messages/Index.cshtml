@model List<AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model!=null)
{
    foreach (var user in Model)
    {
        <a class="text-dark" asp-action="chat" asp-controller="messages" asp-route-receiverId="@user.Id">@user.Fullname</a>
    }
}
else
{
    <h3>Your message box is empty</h3>
}
